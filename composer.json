{
    "name": "msvrtan/hands-on-phpspec-session",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/",
            "spec\\": "spec/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit":                          "~4",
        "mockery/mockery":                          "~0.9",
        "phpspec/phpspec":                          "~2.0",
        "phpmd/phpmd":                              "~2.1",
        "squizlabs/php_codesniffer":                "2.*",
        "fabpot/php-cs-fixer":                      "~1.5",
        "vinkla/climb":                             "^0.8"
    },
    "config": {
        "bin-dir": "bin"
    }
}
